# top-most EditorConfig file
root = true

[src/**]
end_of_line = lf
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[dist/**]
trim_trailing_whitespace = false
insert_final_newline = false

[src/**/vendor/**]
end_of_line = lf
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = false
